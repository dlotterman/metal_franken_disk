[Unit]
Description=Node Exporter
Wants=network-online.target
After=network-online.target

[Service]
User=node_exporter
Group=node_exporter
Type=simple
EnvironmentFile=-/etc/sysconfig/node_exporter
ExecStart=/metal/node_exporter/bin/node_exporter-{{ node_exporter_version }}.linux-amd64/node_exporter --collector.ethtool --collector.perf -–collector.systemd –-collector.processes

[Install]
WantedBy=multi-user.target
